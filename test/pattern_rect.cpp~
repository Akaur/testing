# include <iostream>
# include <stdlib.h>
# include "../include/dxf_2D.h" 

using namespace std;

	int main()
	{
		point p1(0,23), p2(-14,0), p3(14,0);
		/* 3 layer
		   "p_rect.dxf" output filename
		   0 for pattern fill
		   pattern_name = "ansi31"
		   color = 150
		*/
		rectangle bedroom(p1,p3,3,"p_rect.dxf",0,"ansi31",150);
		return 0 ;
	
	}
