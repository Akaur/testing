# include <iostream>
# include <stdlib.h>
# include "../include/dxf_2D.h" 

using namespace std;

	int main()
	{
		point p1(0,23), p2(-14,0), p3(14,0);
		dxf d5("p_circle.dxf");
		/* 5 layer
		   "p_circle.dxf" output filename
		   0 for pattern fill
		   pattern_name = "paisley"
		   200 color code
		*/
		circle c1(p2,p3,5,d,0,"paisley", 200);
		d5.save();
		return 0 ;
	
	}
