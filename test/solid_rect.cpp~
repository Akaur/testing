# include <iostream>
# include <stdlib.h>
# include "../include/dxf_2D.h" 

using namespace std;

	int main()
	{
		point p1(0,23), p2(-14,0), p3(14,0);
		/*   create rectangle with two points
		     2 layer
		     "s_rect.dxf" output filename
		     1 for solid fill flag
		     color code 23 (1-256)
		*/
		rectangle room(p1,p2,2,"s_rect.dxf",1, 190);
		return 0 ;
	
	}
